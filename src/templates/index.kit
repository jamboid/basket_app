<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <!-- Styles -->
    <link rel="stylesheet" href="/css/screen.css">
    <!-- Scripts -->
    <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
    <script type="text/javascript" src="/js/App.js" ></script>

  </head>
  <body>
     <div class="st_App">
      <div class="st_App__inner gd_Group">
        <!-- Product List -->
        <div class="cp_ProductList">
          <div class="cp_ProductList__inner">
            <header class="cp_ProductList__header">
              <h2 class="cp_ProductList__title hd_Han">Products</h2>
            </header>
            <section class="cp_ProductList__items">

              <div class="cp_ProductList__item gd_Group">
                <div class="cp_ProductList__itemInfo">
                  <h3 class="cp_ProductList__itemName">Eggs</h3>
                  <div class="cp_ProductList__itemQuantity">£1.60</div>
                </div>
                <div class="cp_ProductList__itemPrice"></div>
                <div class="cp_ProductList__itemActions">
                  <a href="#" class="cp_ProductList__itemAdd">Add</a>
                </div>
              </div>

              <div class="cp_ProductList__item gd_Group">
                <div class="cp_ProductList__itemInfo">
                  <h3 class="cp_ProductList__itemName">Bread</h3>
                  <div class="cp_ProductList__itemQuantity">£2.00</div>
                </div>
                <div class="cp_ProductList__itemPrice"></div>
                <div class="cp_ProductList__itemActions">
                  <a href="#" class="cp_ProductList__itemAdd">Add</a>
                </div>
              </div>

            </section>
          </div>
        </div>

        <!-- Basket -->
          <div class="cp_Basket">
            <div class="cp_Basket__inner">
              <header class="cp_Basket__header hd_Han">Your Basket</header>
              <section class="cp_Basket__list">
                <div class="cp_Basket__noItems">
                  <h3>Your basket has 0 items</h3>
                </div>
              </section>
              <section class="cp_Basket__total"></section>
              <section class="cp_Basket__actions"></section>
            </div>
          </div>
        <!-- Checkout -->

      </div>
     </div>
  </body>
</html>
